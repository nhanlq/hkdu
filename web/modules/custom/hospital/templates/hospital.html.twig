{#
/**
 * @file hospital.html.twig
 * Default theme implementation to present Hospital entity data.
 *
 * This template is used when viewing Hospital entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_hospital()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('hospital') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
