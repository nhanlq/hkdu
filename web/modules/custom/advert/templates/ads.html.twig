{#
/**
 * @file ads.html.twig
 * Default theme implementation to present Ads data.
 *
 * This template is used when viewing Ads pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_ads()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('ads') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
